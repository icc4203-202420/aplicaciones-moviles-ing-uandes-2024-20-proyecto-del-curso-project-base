    "expo-status-bar": "~2.0.0",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-safe-area-context": "4.12.0",
